{
    "extends": ["eslint:recommended", "google"],
    "plugins": [
        "import"
    ],
    "env": {
		"node": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "script"
    },
    "rules":{
        "linebreak-style": 0,
        "quotes":0,
        "no-console":0,
        "new-cap":0,
        "max-len":0
    }
}